# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GitHub Integration
GITHUB_TOKEN=your_github_token_here
GITHUB_USERNAME=your_github_username

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/multiagent_db
REDIS_URL=redis://localhost:6379/0
MONGODB_URL=mongodb://localhost:27017/multiagent

# Application Settings
APP_NAME=Multi-Agent Development Platform
APP_VERSION=1.0.0
DEBUG=true
LOG_LEVEL=INFO

# Security
SECRET_KEY=your_secret_key_here
JWT_SECRET_KEY=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Web Interface
WEB_HOST=0.0.0.0
WEB_PORT=8080
STREAMLIT_PORT=8501
GRADIO_PORT=8502

# Playwright Configuration
PLAYWRIGHT_BROWSERS_PATH=/ms-playwright
PLAYWRIGHT_HEADLESS=true

# Code Execution Sandbox
SANDBOX_ENABLED=true
SANDBOX_TIMEOUT=30
DOCKER_ENABLED=true

# Monitoring and Logging
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
LOG_FILE_PATH=./logs/app.log

# Development Settings
RELOAD=true
WORKERS=1

# External Services
WEBHOOK_URL=https://your-webhook-url.com
NOTIFICATION_EMAIL=admin@your-domain.com

# Agent Configuration
DEFAULT_MODEL=gpt-4
MAX_TOKENS=4000
TEMPERATURE=0.1
MAX_AGENTS=10

# File Upload Limits
MAX_FILE_SIZE_MB=100
ALLOWED_FILE_TYPES=.py,.js,.ts,.md,.txt,.json,.yaml,.yml,.dockerfile,.sh

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60 